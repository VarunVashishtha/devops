pipeline {
    agent any
    stages {
        stage('Build Stage')
        {
            steps
                {
                    withMaven(maven: 'maven')
                    {
                        sh 'mvn clean compile'
                        sh 'mvn install'
                    }
                }
        }
        
         stage('Testing Stage')
        {
            steps
                {
                    withMaven(maven: 'maven')
                    {
                        sh 'mvn test'
                      
                    }
                }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
