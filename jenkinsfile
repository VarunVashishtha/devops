pipeline {
    agent any
    
    stages {
        stage('Build') {
             steps
                {
                        sh 'mvn clean compile'
                        sh 'mvn install' 
                }
        }
        stage('Test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
